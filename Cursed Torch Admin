local RobloxianLife = loadstring(game:HttpGetAsync("https://cursedtorch.keybase.pub/RobloxianLife/main.lua"))()
local CommandApi = loadstring(game:HttpGetAsync("https://cursedtorch.keybase.pub/CommandApi/main.lua"))()
local StarterGuiApi = loadstring(game:HttpGetAsync("https://cursedtorch.keybase.pub/StarterGuiApi/main.lua"))()
local players = game:GetService("Players")
local localPlayer = players.localPlayer

RobloxianLife:Initialize(gamepassId)

local function strStarts(String,Start)
    return string.sub(String,1,string.len(Start))==Start
end

local function getPlayerByName(name)
    local plrs = {}

    if name == "me" then
        table.insert(plrs, localPlayer)
        return plrs
    elseif name == "others" then
        for i,v in pairs(players:GetPlayers()) do
            if v.Name ~= localPlayer.Name then
                table.insert(plrs, v)
            end
        end
        return plrs
    elseif name == "all" then
        for i,v in pairs(players:GetPlayers()) do
            table.insert(plrs, v)
        end
        return plrs
    elseif name == "random" then
        table.insert(plrs, players:GetPlayers()[math.random(1, #players:GetPlayers())])
        return plrs
    end

    for i,v in pairs(players:GetPlayers()) do
        if strStarts(v.Name, name) then
            table.insert(plrs, v)
        end
    end
    return plrs
end

function randomcolor3() return Color3.fromRGB(math.random(0, 255), math.random(0, 255), math.random(0, 255)) end

CommandApi:AddCommand("ff", function()
    for i,v in pairs(localPlayer.Character:GetChildren()) do 
        if v:IsA("BasePart") then 
            RobloxianLife:SetLocalProperty(v, "Material", Enum.Material.ForceField)
        end
    end

    game:GetService("ReplicatedStorage").AvatarEditor.HatHandle:FireServer("REMOVE")
end)



CommandApi:AddCommand("unff", function()
    for i,v in pairs(localPlayer.Character:GetChildren()) do 
        if v:IsA("BasePart") then 
            RobloxianLife:SetLocalProperty(v, "Material", Enum.Material.Plastic)
        end
    end

    game:GetService("ReplicatedStorage").AvatarEditor.HatHandle:FireServer("REMOVE")
end)

CommandApi:AddCommand("leaderstats", function(name, value)
    name = tostring(name) or "rape"
    value = tostring(value) or "is legal"

    local obj = localPlayer.Character.Animate.run
    RobloxianLife:SetLocalProperty(obj, "Value", value)
    RobloxianLife:SetLocalProperty(obj, "Name", name)
    RobloxianLife:SetLocalProperty(obj, "Parent", localPlayer.leaderstats)
    wait()
    localPlayer.Character.Head:Destroy()
end)

local raintagRunning = false
CommandApi:AddCommand("raintag", function()
    if not raintagRunning then
        raintagRunning = true
        local died = false

        localPlayer.Character.Humanoid.Died:connect(function() died = true; raintagRunning = false end)

        while not died and raintagRunning do 
            for i = 0,1,0.01 do 
                if died or not raintagRunning then break end
                if localPlayer.Character.Head:FindFirstChild("NametagTemplate") then
                    RobloxianLife:SetLocalProperty(localPlayer.Character.Head.NametagTemplate.TagText, "TextColor3", Color3.fromHSV(i,1,1))
                end
                wait(.05)          
            end    
        end
    end
end)

CommandApi:AddCommand("shirt", function(id)
    id = "rbxassetid://"..tostring(id)

    RobloxianLife:SetLocalProperty(localPlayer.Character.Shirt, "ShirtTemplate", id)
end)

CommandApi:AddCommand("hat2", function(id)
    id = "rbxassetid://"..tostring(id)

    RobloxianLife:SetLocalProperty(localPlayer.Character.TheSpookHood.Handle.SpecialMesh, "TextureId", id)
end)

CommandApi:AddCommand("hat", function(id)
    id = "rbxassetid://"..tostring(id)

    RobloxianLife:SetLocalProperty(localPlayer.Character.TheSpookHood.Handle.SpecialMesh, "MeshId", id)
end)

CommandApi:AddCommand("pants", function(id)
    id = "rbxassetid://"..tostring(id)

    RobloxianLife:SetLocalProperty(localPlayer.Character.Pants, "PantsTemplate", id)
end)

local rainbowRunning = false
CommandApi:AddCommand("rainbow", function()
    if not rainbowRunning then
        rainbowRunning = true
        local died = false

        localPlayer.Character.Humanoid.Died:connect(function() died = true; rainbowRunning = false end)

        while not died and rainbowRunning do 
            for i = 0,1,0.01 do 
                if died or not rainbowRunning then break end
                for _,v in pairs(localPlayer.Character:GetChildren()) do
                    if died or not rainbowRunning then break end

                    if v:IsA("BasePart") then
                        RobloxianLife:SetLocalProperty(v, "Color", Color3.fromHSV(i,1,1))          
                    end
                end
                wait(.05)          
            end    
        end
    end
end)

CommandApi:AddCommand("unrainbow", function()
    if rainbowRunning then
        rainbowRunning = false
    end
end)

CommandApi:AddCommand("color", function(r, g, b)
    r = tonumber(r) or 255
    g = tonumber(g) or 255
    b = tonumber(b) or 255

    for i,v in pairs(localPlayer.Character:GetChildren()) do
        RobloxianLife:SetLocalProperty(v, "Color", Color3.fromRGB(r, g, b))
    end
end)

CommandApi:AddCommand("give", function(plr)
    for i,v in pairs(getPlayerByName(plr)) do
        if localPlayer.Character:FindFirstChildOfClass("Tool") then
            local tool = localPlayer.Character:FindFirstChildOfClass("Tool")

            RobloxianLife:SetLocalProperty(tool, "Parent", v.Character)
        end
    end
end)

StarterGuiApi:ChatMakeSystemMessage(" ")
StarterGuiApi:ChatMakeSystemMessage(" - Robloxian Life - Cursed Torch Admin loaded.", Color3.new(255, .0, .0), Enum.Font.Code, Enum.FontSize.Size28)
StarterGuiApi:ChatMakeSystemMessage(" ")
